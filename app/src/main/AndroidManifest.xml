<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajatiit.admin_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/CustomizedAppTheme">
        <activity
            android:name=".AdminMainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intefaces.teacherInterface.TeacherInterface" />
        <activity android:name=".intefaces.batchInterface.BatchInterface" />
        <activity android:name=".intefaces.classroomInterface.ClassroomInterface" />
        <activity android:name=".intefaces.classroomInterface.AssignTeacherOrBatch" />
        <activity android:name=".intefaces.roomInterface.RoomInterface" />
        <activity android:name=".TimeTableDisplay.ShowIndividualSlot"></activity>
    </application>

</manifest>